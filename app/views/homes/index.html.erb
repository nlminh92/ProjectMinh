<div id="page-wrapper">
  <% if user_signin_not_active %>
    <% if current_user.type_user == 0 %>
      <p>Brand</p>
    <% elsif current_user.type_user == 1 %>
      <p>Retailler</p>
    <% else %>
      <p>Admin</p>
    <% end %>
    <p>Account not active<p>
  <% else %>
    <% if current_user.type_user == 0 %>
      <p>Brand</p>
    <% elsif current_user.type_user == 1 %>
      <p>Retailler</p>
    <% else %>
      <p>Admin</p>
    <% end %>
    <h1>Account actived</h1>
  <% end %>
</div>
